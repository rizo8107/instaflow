{
  "name": "instaflow-frontend",
  "description": "Instagram Automation Platform Frontend with visual workflow builder",
  "version": "1.0.0",
  "repository": "https://github.com/rizo8107/instaflow.git",
  "services": {
    "frontend": {
      "env": {
        "VITE_META_APP_ID": "${META_APP_ID}",
        "VITE_REDIRECT_URI": "${REDIRECT_URI}",
        "VITE_API_BASE_URL": "${API_BASE_URL}",
        "VITE_N8N_WEBHOOK_URL": "${VITE_N8N_WEBHOOK_URL}"
      },
      "resources": {
        "memory": "1024M",
        "cpu": 1
      }
    }
  },
  "volumes": {
    "data": {
      "mountPath": "/app/data"
    }
  }
}
